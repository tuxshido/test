{
  "branchNames": {
    "develop": "develop",
    "main": "f",
    "release": "g",
    "issueBranchTemplate": "feature/iss/#<ISSUE_NUMBER>"
  },
  "start": {
    "fromDevelopOnly": true,
    "pullBeforeAlways": true,
    "checkoutToFeature": true
  },
  "finish": {
    "checkOutToDevelop": false,
    "checkMergeConflict": true
  },
  "git": {
    "remoteAlias": "origin"
  }
}